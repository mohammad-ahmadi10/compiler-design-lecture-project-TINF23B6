int main := () {
  int x = 2;
  int y = 3;

  switch (x) {
    case 1:
      int scopedVar = 10;
      print(scopedVar);
    case 2:
      int scopedVar = 20;
      print(scopedVar);
    case "hello":
      print("x is a greeting");
    default:
      print("x is something else");
  }

  switch (y) {
    case 3:
      int scopedVar = 30;
      print(scopedVar);
    default:
      int scopedVar = 100;
      print(scopedVar);
  }
}